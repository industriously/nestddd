model accounts {
  /// @format uuid
  id         String   @id @default(uuid())
  sub        String
  oauth_type String
  /// @format email
  email      String   @unique
  username   String
  created_at DateTime @default(now())
  updated_at DateTime @default(now())
  is_deleted Boolean  @default(false)

  @@unique([sub, oauth_type])
}
